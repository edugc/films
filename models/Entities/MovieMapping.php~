<?php
namespace models\Entities;
/**
 * @Entity
 * @Table(name="peliculas")
 */

class MovieMapping
{
    /**
     * @Id @Column(type="string",  name="titulo")
     */
    private $title;
    /**
     * @Column(type="string",  name="sinopsis")
     */
    private $sinopsis;
    /**
     * @Column(type="integer",  name="anio")
     */
    private $year;
    /**
     * @Column(type="string",  name="nacionalidad")
     */
    private $country;
    /**
     * @Column(type="string",  name="genero")
     */
    private $genero;
    /**
     * @Column(type="string",  name="cartel")
     */
    private $cartel;

    /**
     * Set title
     *
     * @param string $title
     *
     * @return MovieMapping
     */
    public function setTitle($title)
    {
        $this->title = $title;

        return $this;
    }

    /**
     * Get title
     *
     * @return string
     */
    public function getTitle()
    {
        return $this->title;
    }

    /**
     * Set sinopsis
     *
     * @param string $sinopsis
     *
     * @return MovieMapping
     */
    public function setSinopsis($sinopsis)
    {
        $this->sinopsis = $sinopsis;

        return $this;
    }

    /**
     * Get sinopsis
     *
     * @return string
     */
    public function getSinopsis()
    {
        return $this->sinopsis;
    }

    /**
     * Set year
     *
     * @param string $year
     *
     * @return MovieMapping
     */
    public function setYear($year)
    {
        $this->year = $year;

        return $this;
    }

    /**
     * Get year
     *
     * @return string
     */
    public function getYear()
    {
        return $this->year;
    }

    /**
     * Set country
     *
     * @param string $country
     *
     * @return MovieMapping
     */
    public function setCountry($country)
    {
        $this->country = $country;

        return $this;
    }

    /**
     * Get country
     *
     * @return string
     */
    public function getCountry()
    {
        return $this->country;
    }

    /**
     * Set genero
     *
     * @param string $genero
     *
     * @return MovieMapping
     */
    public function setGenero($genero)
    {
        $this->genero = $genero;

        return $this;
    }

    /**
     * Get genero
     *
     * @return string
     */
    public function getGenero()
    {
        return $this->genero;
    }

    /**
     * Set cartel
     *
     * @param string $cartel
     *
     * @return MovieMapping
     */
    public function setCartel($cartel)
    {
        $this->cartel = $cartel;

        return $this;
    }

    /**
     * Get cartel
     *
     * @return string
     */
    public function getCartel()
    {
        return $this->cartel;
    }
}
